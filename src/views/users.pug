extends common.pug

block content
    h1(class='text-center') All users
    br
    a(href='/users/create' class="d-flex justify-content-center")
        button(class='btn btn-primary ml-2 btn-sm') Create new user
    form(action='/users/search', method='GET' class='text-center')
        input(type='text', name='q')
        button Search
    ol(class='text-center')
        each key,val in users
            li= val + ' : ' + key
    a(href='/commands/list' class="d-flex justify-content-center")
        button(class='btn btn-primary ml-2 btn-sm') Command list
